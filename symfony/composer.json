{
    "name": "vendor/symfony",
    "description": "This project was created with apie/apie-project-starter",
    "license": "proprietary",
    "keywords": [
        "apie",
        "rest",
        "api",
        "openapi"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "apie/meta-maximum": "1.0.0.x-dev",
        "apie/apie-bundle": "1.0.0.x-dev",
        "apie/ftp-server": "1.0.0.x-dev",
        "doctrine/doctrine-bundle": "^2.10",
        "symfony/console": "6.*|7.*",
        "symfony/framework-bundle": "6.*|7.*",
        "symfony/runtime": "6.*|7.*",
        "symfony/dotenv": "6.*|7.*",
        "symfony/flex": "^2",
        "symfony/yaml": "6.1.*|7.*",
        "symfony/security-bundle": "6.*|7.*",
        "apie/cms-layout-graphite": "1.0.0.x-dev",
        "symfony/twig-bundle": "6.*|7.*"
    },
    "require-dev": {
        "apie/apie-phpstan-rules": "1.0.0.x-dev",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0",
        "symfony/debug-bundle": "6.*|7.*",
        "symfony/monolog-bundle": "^3.0|7.*",
        "symfony/stopwatch": "6.*|7.*",
        "symfony/web-profiler-bundle": "6.*|7.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Domains\\": "domains/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "DomainTests\\": "domain-tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "apie/apie-common-plugin": true,
            "symfony/runtime": true,
            "symfony/flex": true
        }
    },
    "conflict": {
        "symfony/twig-bundle": "v6.4.0-BETA1"
    }
}
